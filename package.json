{
  "name": "@gebruederheitz/sticky-parameters",
  "version": "2.3.2",
  "description": "Forward certain URL parameters to allowlisted or non-denylisted links on the current site.",
  "module": "dist/index.m.js",
  "main": "dist/sticky-parameters.umd.js",
  "browser": "dist/sticky-parameters.umd.js",
  "author": "Andreas Maros <andreas.maros@gebruderheitz.de>",
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gebruederheitz/sticky-parameters.git"
  },
  "keywords": [
    "url",
    "parameter",
    "tracking",
    "cross-domain",
    "query"
  ],
  "bugs": {
    "url": "https://github.com/gebruederheitz/sticky-parameters/issues"
  },
  "homepage": "https://github.com/gebruederheitz/sticky-parameters#readme",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "core-js": "^3.18.3",
    "jsuri": "^1.3.1"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv demo --no-clear",
    "host": "sirv demo --no-clear --host",
    "lint": "eslint src/**/*.js",
    "prettier:check": "prettier -c --config .prettierrc src/",
    "prettier:fix": "prettier -w --config .prettierrc src/",
    "release": "release-it"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.12.16",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "release-it": "^14.12.1",
    "rollup": "^2.39.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.0"
